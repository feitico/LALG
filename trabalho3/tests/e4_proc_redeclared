program p4;
var a : integer;
var b : real;
procedure meu_proc(x: real);
begin
    x := 2.5;
end;

procedure meu_proc1(x: integer; y: real);
begin
    x := 2;
    y := 3.5;
end;

{Procedimento redeclarado}
procedure meu_proc(x: real);
begin
    x := 4;
end;


begin
    a := 10;
    b := 5.7;
    meu_proc1(a; b);
    meu_proc(b);
end.

